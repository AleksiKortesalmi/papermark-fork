VERSION 1

NODE endpoint
SQL >
    %
    SELECT
        country,
        city,
        browser,
        os,
        device
    FROM
        video_views__v1
    WHERE
        view_id = {{ String(viewId, required=True) }}
        AND timestamp >= {{ Int64(since, required=True) }}
    ORDER BY
        view_id, timestamp ASC
    LIMIT 1 